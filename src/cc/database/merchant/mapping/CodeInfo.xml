<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cc.database.merchant.mapping.codeInfo">
    <select id="getCodeInfoById" parameterType="long" resultMap="codeInfoResult">
        SELECT * FROM codeinfo where id=#{id}
    </select>
    <select id="insertCodeInfo" parameterType="java.util.HashMap" resultMap="codeInfoResult">
        INSERT INTO codeinfo (id,agentid) VALUES (#{id},#{agentid})
    </select>
    <resultMap type="cc.database.merchant.CodeInfo" id="codeInfoResult">
        <id property="id" column="id" jdbcType="BIGINT" />
        <result property="agentId" column="agentId" jdbcType="VARCHAR" />
        <result property="makeDate" column="makeDate" jdbcType="TIMESTAMP" />
    </resultMap>
</mapper>